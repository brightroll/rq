<%- status = qc.status -%>
status: <%= status %>
<%- if status != "DOWN" -%>
uptime: <%= qc.uptime %>

<%- messages = qc.num_messages -%>
prep: <%= messages['prep'] %>
que: <%= messages['que'] %>
run: <%= messages['run'] %>

done: <%= messages['done'] %>
err: <%= messages['err'] %>
relayed: <%= messages['relayed'] %>
<%- end -%>
