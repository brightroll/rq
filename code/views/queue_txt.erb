<%="uptime: #{qc.uptime}"%>
<% status = qc.status %>
<%="admin_status: #{status[0]}"%>
<%="oper_status: #{status[1]}"%>

<% messages = qc.num_messages %>
<%="prep: #{messages['prep']}"%>
<%="que: #{messages['que']}"%>
<%="run: #{messages['run']}"%>

<%="done: #{messages['done']}"%>
<%="err: #{messages['err']}"%>
<%="relayed: #{messages['relayed']}"%>
