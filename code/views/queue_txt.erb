<% status = qc.status %>
<%="admin_status: #{status[0]}"%>
<%="oper_status: #{status[1]}"%>
<% if status[1] != "DOWN" %>
<%="uptime: #{qc.uptime}"%>

<% messages = qc.num_messages %>
<%="prep: #{messages['prep']}"%>
<%="que: #{messages['que']}"%>
<%="run: #{messages['run']}"%>

<%="done: #{messages['done']}"%>
<%="err: #{messages['err']}"%>
<%="relayed: #{messages['relayed']}"%>
<% end %>
