<%- status = qc.status -%>
{"status":"<%= status -%>"
<%- if status != 'DOWN' -%>
,"uptime":<%= qc.uptime -%>
<%- num_messages = qc.num_messages -%>
,"prep_size":<%= num_messages['prep'] -%>
,"que_size":<%= num_messages['que'] -%>
,"run_size":<%= num_messages['run'] -%>
,"done_size":<%= num_messages['done'] -%>
,"err_size":<%= num_messages['err'] -%>
,"relayed_size":<%= num_messages['relayed'] -%>
<%- end -%>
}
